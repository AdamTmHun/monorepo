{
	"name": "@inlang/resolve-plugins",
	"type": "module",
	"version": "0.0.0",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": "./dist/index.js",
		"./test": "./dist/test/index.js"
	},
	"files": [
		"./dist",
		"./src"
	],
	"scripts": {
		"build": "tsc --build ./tsconfig.build.json",
		"dev": "tsc -p ./tsconfig.build.json --watch",
		"test": "tsc --noEmit && vitest run --passWithNoTests --coverage",
		"lint": "eslint ./src",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./.turbo ./node_modules"
	},
	"dependencies": {
		"@lix-js/fs": "*",
		"@inlang/plugin": "*",
		"@inlang/result": "*",
		"deepmerge-ts": "^5.1.0",
		"ts-dedent": "^2.2.0",
		"@sinclair/typebox": "^0.30.0"
	},
	"devDependencies": {
		"@vitest/coverage-v8": "^0.33.0",
		"tsd": "^0.25.0",
		"vitest": "^0.33.0"
	},
	"license": "Apache-2.0"
}
