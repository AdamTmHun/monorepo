{
	"name": "@inlang/sdk",
	"type": "module",
	"version": "0.0.0",
	"license": "Apache-2.0",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": "./dist/index.js"
	},
	"files": [
		"./dist",
		"./src"
	],
	"scripts": {
		"postinstall": "patch-package",
		"build": "tsc --build ./tsconfig.build.json",
		"dev": "tsc -p ./tsconfig.build.json --watch",
		"test": "tsc --noEmit && vitest run --passWithNoTests --coverage",
		"lint": "eslint ./src",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./.turbo ./node_modules"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"dependencies": {
		"@inlang/project-config": "*",
		"@inlang/language-tag": "*",
		"@inlang/lint": "*",
		"@inlang/messages": "*",
		"@inlang/message": "*",
		"@inlang/package": "*",
		"@inlang/plugin": "*",
		"@inlang/result": "*",
		"@sinclair/typebox": "^0.31.0",
		"@solid-primitives/map": "0.4.7",
		"deepmerge-ts": "^5.1.0",
		"throttle-debounce": "^5.0.0",
		"ts-dedent": "2.2.0"
	},
	"devDependencies": {
		"patch-package": "6.5.1",
		"@vitest/coverage-v8": "^0.33.0",
		"@lix-js/fs": "*",
		"tsd": "^0.25.0",
		"vitest": "^0.33.0"
	}
}
