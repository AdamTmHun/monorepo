{
	"name": "@inlang/plugin-i18next",
	"version": "3.0.2",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"files": [
		"./dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"postinstall": "patch-package",
		"dev": "inlang package build --entry ./src/index.ts --outdir ./dist --watch",
		"build": "inlang package build --entry ./src/index.ts --outdir ./dist",
		"test": "tsc --noEmit && vitest run --passWithNoTests --coverage",
		"lint": "eslint ./src",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./.turbo ./node_modules"
	},
	"devDependencies": {
		"@inlang/cli": "*",
		"@inlang/plugin": "*",
		"@size-limit/preset-small-lib": "^8.2.4",
		"@types/flat": "^5.0.2",
		"@types/lodash.merge": "^4.6.7",
		"@types/parsimmon": "1.10.6",
		"@vitest/coverage-v8": "^0.33.0",
		"patch-package": "6.5.1",
		"flat": "^5.0.2",
		"parsimmon": "^1.18.1",
		"typescript": "^5.1.3"
	}
}
