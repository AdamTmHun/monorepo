{
	"name": "@inlang/plugin-standard-lint-rules",
	"version": "3.0.0",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"files": [
		"./dist"
	],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "inlang module build --entry ./src/index.ts --outdir ./dist --watch",
		"build": "inlang module build --entry ./src/index.ts --outdir ./dist",
		"test": "tsc --noEmit && vitest run --passWithNoTests --coverage",
		"lint": "eslint ./src",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./.turbo ./node_modules"
	},
	"dependencies": {
		"@inlang/plugin": "*"
	},
	"devDependencies": {
		"@inlang/sdk": "*"	,
		"@inlang/cli": "*",
		"concurrently": "^8.0.1",
		"tsd": "0.28.1",
		"typescript": "5.2.2",
		"vitest": "0.34.3",
		"@vitest/coverage-v8": "0.34.3"
	}
}
