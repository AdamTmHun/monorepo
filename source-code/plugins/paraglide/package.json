{
	"name": "@inlang/plugin-paraglide",
	"keywords": [
		"inlang",
		"i18n",
		"internationalization",
		"L10n",
		"localization",
		"translation"
	],
	"type": "module",
	"version": "0.11.8",
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"./dist"
	],
	"scripts": {
		"dev": "inlang module build --entry ./src/index.ts --outdir ./dist --watch",
		"build": "inlang module build --entry ./src/index.ts --outdir ./dist",
		"test": "tsc --noEmit && vitest run --passWithNoTests --coverage",
		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./.turbo ./node_modules"
	},
	"devDependencies": {
		"@inlang/cli": "*",
		"@inlang/plugin": "*",
		"chevrotain": "^10.5.0",
		"typescript": "^4.9.5",
		"vitest": "^0.29.1"
	},
	"license": "Apache-2.0"
}
