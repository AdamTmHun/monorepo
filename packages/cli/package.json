    "jest": "^27.0.6",
    "jest-esm-transformer": "^1.0.0",
    "ts-jest": "^27.0.4",
    "@inlang/database": "file:../database"
  },
  "dependencies": {
    "@supabase/supabase-js": "^1.23.0",
    "commander": "^2.20.0",
    "dotenv": "^10.0.0"
  }
