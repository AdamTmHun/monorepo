{
  "name": "inlang-cli",
  "version": "1.0.0",
  "description": "cli for inlang.",
  "author": "doofin <8177dph@gmail.com>",
  "keywords": [
    "typescript"
  ],
  "license": "MIT",
  "main": "./dist/bin.js",
  "bin": "./dist/bin.js",
  "files": [
    "dist/**/*",
    "!dist/**/*.test.js"
  ],
  "scripts": {
