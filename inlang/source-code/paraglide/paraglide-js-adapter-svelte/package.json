{
	"name": "@inlang/paraglide-js-adapter-svelte",
	"type": "module",
	"version": "0.1.0",
	"license": "Apache-2.0",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"import": "./dist/compiled-output/runtime.js",
			"types": "./dist/compiled-output/runtime$.d.ts"
		},
		"./messages": {
			"import": "./dist/compiled-output/messages.js",
			"types": "./dist/compiled-output/messages$.d.ts"
		}
	},
	"bin": {
		"paraglide-svelte": "./bin/run.js"
	},
	"files": [
		"./dist",
		"./bin"
	],
	"scripts": {
		"build": "tsc --build",
		"dev": "tsc --watch",
		"test": "tsc --noEmit && vitest run --coverage",
		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./.turbo ./node_modules"
	},
	"dependencies": {
		"@inlang/paraglide-js": "*"
	},
	"__CONTAINS DUPLICATE DEPENDENCIES OF THE EXAMPLE DUE TO": "NPM BUG https://github.com/npm/cli/issues/6874",
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "2.4.2",
		"@inlang/paraglide-js": "*",
		"svelte": "4.0.5",
		"vite": "4.4.5",
		"@vitest/coverage-v8": "0.34.3",
		"typescript": "5.2.2",
		"vitest": "0.34.3"
	}
}
