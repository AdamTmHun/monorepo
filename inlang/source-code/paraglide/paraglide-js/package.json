{
	"name": "@inlang/paraglide-js",
	"type": "module",
	"version": "0.1.0",
	"license": "Apache-2.0",
	"publishConfig": {
		"access": "public"
	},
	"exports": {
		".": {
			"import": "./dist/compiled-output/runtime.js",
			"types": "./dist/compiled-output/runtime$.d.ts"
		},
		"./messages": {
			"import": "./dist/compiled-output/messages.js",
			"types": "./dist/compiled-output/messages$.d.ts"
		},
		"./messages/*": {
			"import": "./dist/compiled-output/lang/*.js",
			"types": "./dist/compiled-output/lang/*$.d.ts"
		}
	},
	"bin": {
		"paraglide-js": "./bin/run.js"
	},
	"files": [
		"./dist",
		"./bin"
	],
	"scripts": {
		"build": "tsc --build",
		"dev": "tsc --watch",
		"pretest": "npm run build && npm run build -w @inlang/paraglide-js-example",
		"test": "tsc --noEmit && vitest run --coverage",
		"lint": "eslint ./src --fix",
		"format": "prettier ./src --write",
		"clean": "rm -rf ./dist ./.turbo ./node_modules"
	},
	"dependencies": {
		"@inlang/sdk": "*",
		"commander": "11.1.0",
		"consola": "3.2.3",
		"dedent": "1.5.1"
	},
	"devDependencies": {
		"@rollup/plugin-terser": "0.4.3",
		"@rollup/plugin-virtual": "3.0.1",
		"@ts-morph/bootstrap": "0.20.0",
		"@types/minimist": "1.2.3",
		"@vitest/coverage-v8": "0.34.3",
		"rollup": "3.29.1",
		"typescript": "5.2.2",
		"vitest": "0.34.3"
	}
}
