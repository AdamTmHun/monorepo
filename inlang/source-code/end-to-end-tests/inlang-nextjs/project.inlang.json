		"./../../plugins/i18next/dist/index.js",
		"./../../lint-rules/emptyPattern/dist/index.js",
		"./../../lint-rules/identicalPattern/dist/index.js",
		"./../../lint-rules/missingTranslation/dist/index.js",
		"./../../lint-rules/messageWithoutSource/dist/index.js"
	],
	"settings": {
		"plugin.inlang.i18next": {
			"pathPattern": {
				"client-page": "app/i18n/locales/{languageTag}/client-page.json",
